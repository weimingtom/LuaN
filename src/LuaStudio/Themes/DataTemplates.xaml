<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LuaStudio"
                    xmlns:vw="clr-namespace:LuaStudio.Views"
                    xmlns:vm="clr-namespace:LuaStudio.ViewModels"
                    xmlns:vw_tools="clr-namespace:LuaStudio.Views.Tools"
                    xmlns:vm_tools="clr-namespace:LuaStudio.ViewModels.Tools"
                    >

    <DataTemplate DataType="{x:Type vm_tools:SnippetsToolViewModel}">
        <vw_tools:SnippetsToolView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm_tools:InteractiveLuaToolViewModel}">
        <vw_tools:InteractiveLuaToolView />
    </DataTemplate>
    
</ResourceDictionary>